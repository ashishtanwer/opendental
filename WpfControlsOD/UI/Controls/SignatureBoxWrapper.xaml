<UserControl x:Class="WpfControls.UI.SignatureBoxWrapper"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfControls.UI"
             xmlns:oduiodb="clr-namespace:OpenDental.UI;assembly=OpenDentBusiness"
             mc:Ignorable="d" 
            x:Name="userControl" Background="White"
            d:DesignWidth="362" d:DesignHeight="79">
    <UserControl.Style>
        <Style>
            <Setter Property="local:SignatureBoxWrapper.HorizontalAlignment" Value="Left"/>
            <Setter Property="local:SignatureBoxWrapper.VerticalAlignment" Value="Top"/>
        </Style>
    </UserControl.Style>
    <UserControl.Template>
        <ControlTemplate TargetType="UserControl">
            <ContentPresenter/>
        </ControlTemplate>
    </UserControl.Template>
    <Grid x:Name="grid" Background="White">
        <WindowsFormsHost Name="windowsFormsHost">
            
        </WindowsFormsHost>
    </Grid>
</UserControl>