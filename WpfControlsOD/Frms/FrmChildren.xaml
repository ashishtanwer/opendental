<local:FrmODBase x:Class="OpenDental.FrmChildren"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="525" Height="563" 
             Text="Children" MinMaxBoxes="False">
	<Grid Background="#FFFCFDFE" >
		<ui:Grid x:Name="gridMain" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="12,10,110,10" Title="Children" />
		<ui:Button x:Name="butAdd" HorizontalAlignment="Right" Width="76" Margin="0,10,16,0" Text="_Add" Click="butAdd_Click" RenderTransformOrigin="0.543,1.257" Icon="Add" Height="24" />
		<ui:CheckBox x:Name="checkShowHidden" Margin="0,56,6,0" HorizontalAlignment="Right" Width="92" Height="20" Text="Show Hidden" Click="checkShowHidden_Click"/>
		<ui:Button x:Name="butOK" Margin="0,0,16,10" Text="OK" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Click="butOK_Click" />
	</Grid>
</local:FrmODBase>