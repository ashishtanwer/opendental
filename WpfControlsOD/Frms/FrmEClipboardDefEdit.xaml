		<ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Margin="0,0,10,10" Text="_Save" Click="butSave_Click" />
		<ui:Button x:Name="butDelete" Width="84" VerticalAlignment="Bottom" Height="24" Margin="10,0,0,10" Text="Delete" Icon="DeleteX" Click="butDelete_Click" />
		<ui:TextBox x:Name="textName" Height="20" Width="285" Margin="122,26,0,0"/>
		<ui:Label Height="18" Margin="34,27,0,0" Text="Name" Width="88" VAlign="Center" HAlign="Right" />
		<ui:Label Height="18" Margin="6,52,0,0" Text="Patient Instructions" Width="116" VAlign="Center" HAlign="Right"/>
		<ui:TextBox x:Name="textPatInstructions" Height="70" Width="285" Margin="122,51,0,0" IsMultiline="True"/>
	</Grid>
</local:FrmODBase>