		<ui:Button x:Name="butBlank" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Margin="0,0,27,300" Text="Blank" Click="butBlank_Click"  />
		<ui:Label x:Name="labelAdd" Width="55" Height="18" Margin="374,14,0,0" Text="or add" />
	</Grid>
</local:FrmODBase>